void fun(int m,int k, int a[])
{
	int i,n,data;
	data=m+1;
	n=0;
	while(n<k)
	{
		for(i=2;i<data;i++)
			if(data%i==0)
				break;
			if(i>=data)
				a[n++]=data;
				data++;
	}
	for(i=0;i<k;i++)
		printf("大于%d的第%d个素数为%d\n",m,i+1,a[i]);
}
int main()
{
	int m,k,a[20];
	scanf("%d%d",&m,&k);
	fun(m,k,a);
	return 0;
}
