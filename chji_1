#include <stdio.h>
int fun(int a[])
{
	int i,n=0;
	for(i=1;i<1000;i++)
		if((i%7==0&&i%11!=0)||(i%7!=0&&i%11==0))
			a[n++]=i;
	return n;
}
int main()
{
	int a[1000],i,n;
	for(i=0;i<1000;i++)
		a[i]=i+1;
	n=fun(a);
	for(i=0;i<n;i++)
	{
		printf("a[%3d]=%3d ",i,a[i]);
		if((i+1)%10==0)
			printf("\n");
	}
	printf("\n%d\n",n);
	return 0;
}
